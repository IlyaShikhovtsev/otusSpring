<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questionDao" class="ru.shikhovtsev.dao.QuestionCsvDao">
        <constructor-arg value="questions.csv"/>
    </bean>
    <bean id="questionService" class="ru.shikhovtsev.service.QuestionServiceImpl">
        <constructor-arg ref="questionDao"/>
    </bean>

    <bean id="consoleService" class="ru.shikhovtsev.service.ConsoleService"/>
    <bean class="ru.shikhovtsev.service.KnowledgeServiceImpl">
        <constructor-arg ref="consoleService"/>
        <constructor-arg ref="questionService"/>
    </bean>
</beans>
